<?xml version="1.0" encoding="UTF-8"?>
<project-env xmlns="ch.repolevedavaj.projectenv"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="ch.repolevedavaj.projectenv src/main/resources/ch/repolevedavaj/projectenv/project-env.xsd">
    <tools>
        <directory>.tools</directory>
        <installers>
            <jdk>
                <name>jdk</name>
                <download-uris>
                    <macos>https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.9.1%2B1/OpenJDK11U-jdk_x64_mac_hotspot_11.0.9.1_1.tar.gz</macos>
                    <windows>https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.9.1%2B1/OpenJDK11U-jdk_x86-32_windows_hotspot_11.0.9.1_1.zip</windows>
                    <linux>https://github.com/AdoptOpenJDK/openjdk11-binaries/releases/download/jdk-11.0.9.1%2B1/OpenJDK11U-jdk_x64_linux_hotspot_11.0.9.1_1.tar.gz</linux>
                </download-uris>
            </jdk>
            <maven>
                <name>maven</name>
                <download-uris>
                    <system-independent>https://downloads.apache.org/maven/maven-3/3.6.3/binaries/apache-maven-3.6.3-bin.tar.gz</system-independent>
                </download-uris>
            </maven>
        </installers>
    </tools>
